{
  "name": "hhackathon-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=18.18",
    "pnpm": ">=8"
  },
  "scripts": {
    "dev:mobile": "pnpm --filter @hhackathon/mobile start",
    "lint": "pnpm --filter ./apps/mobile typecheck",
    "api:dev": "uv run --project apps/api -- uvicorn api.main:app --reload",
    "vision:run": "uv run --project apps/api -- python -m api.workers.vision_supervisor"
  }
}
